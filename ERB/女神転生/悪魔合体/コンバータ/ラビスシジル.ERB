;--------------------------------------------------
;合体結果を偽典女神転生仕様にコンバートする
;ARG コンバートする前の合体結果(NO)
;--------------------------------------------------
@CONVERT_GIGATEN, ARG
#LOCALSSIZE 20
#DIM 검색후보
#DIM 검색결과
IF LOCALS:0 == ""
	LOCALS:0  = {[[캐릭터:실키]]}　　　　→　{[[캐릭터:블루벨]]}
	LOCALS:1  = {[[캐릭터:백낭자]]}　　→　{[[캐릭터:카야노히메]]}
	LOCALS:2  = {[[캐릭터:쵸우케신]]}　　→　{[[캐릭터:네레이드]]}
	LOCALS:3  = {[[캐릭터:메즈키]]}　　　　　→　{[[캐릭터:누리카베]]}
	LOCALS:4  = {[[캐릭터:사티]]}　　　　　→　{[[캐릭터:아나히타]]}
	LOCALS:5  = {[[캐릭터:오키쿠무시]]}　　　→　{[[캐릭터:오사키여우]]}
	LOCALS:6  = {[[캐릭터:도어마스]]}　　　→　{[[캐릭터:쿠즈노하]]}
	LOCALS:7  = {[[캐릭터:오바리욘]]}　　　→　{[[캐릭터:엔티티]]}
;	LOCALS:8  = {[[캐릭터:라이라]]}　　　　　→　{[[캐릭터:라파엘라]]}
;	LOCALS:9  = {[[캐릭터:스로네]]}　　　　　→　{[[캐릭터:파누엘]]}
;	LOCALS:10  = {[[캐릭터:프레이아]]}　　　　→　{[[캐릭터:닌후르삭]]}
	LOCALS:11  = {[[캐릭터:오퀴페테]]}　　　→　{[[캐릭터:포다르게]]}
	LOCALS:12  = {[[캐릭터:아에로]]}　　　　　→　{[[캐릭터:니코토에]]}
	LOCALS:13  = {[[캐릭터:필방]]}　　　　　→　{[[캐릭터:우피오르]]}
	LOCALS:14  = {[[캐릭터:웜]]}　　　　　　→　{[[캐릭터:아펩]]}
	LOCALS:15  = {[[캐릭터:매드가서]]}　　→　{[[캐릭터:다크니스]]}
	LOCALS:16  = {[[캐릭터:사라스바티]]}　　→　{[[캐릭터:브리이드]]}
	LOCALS:17  = {[[캐릭터:프린시펄리티]]}　→　{[[캐릭터:아브디엘]]}
	LOCALS:18  = {[[캐릭터:모코이]]}　　　　　　→　{[[캐릭터:케우케겐]]}
	LOCALS:19  = {[[캐릭터:온모라키]]}　　　　　　→　{[[캐릭터:금시조]]}
ENDIF
FOR LOCAL,0,VARSIZE("LOCALS")
	검색후보 = TOINT(AUTO_SPLIT(LOCALS:LOCAL,"　",0))
	검색결과 = TOINT(AUTO_SPLIT(LOCALS:LOCAL,"　→　",1))
	SIF ARG != 검색후보
		CONTINUE
	RETURN 검색결과
NEXT
RETURN ARG
