@CAN_ENTER_28,ARG,ARG:1
RESULT = 1
SELECTCASE DA:ARG:(ARG:1)
	CASE 12
		;監視塔入り입
		IF BASE:MASTER:지혜 >= 10
			RETURN 1
		ELSE
			RETURN 0
		ENDIF
	CASE 11002
		IF 던전플래그:[[던전:팩토리]]:7 == 2
			RETURN 1
		ELSEIF 던전플래그:[[던전:팩토리]]:7 == 1
			CALL MESSAGE_WINDOW_D , "" , "＞지하통로의 열쇠를 이용해 문을 열었다"
			던전플래그:[[던전:팩토리]]:7 = 2
			RETURN 1
		ELSE
			CALL MESSAGE_WINDOW_D , "" , "＞문은 닫혀있다"
			RETURN 0
		ENDIF
	CASE 11012, 11022, 11032
		IF !GETBIT(던전플래그:[[던전:팩토리]]:14 , DA:ARG:(ARG:1) / 10 % 10 -1)
			CALL MESSAGE_WINDOW_D , "" , "＞전자록이 걸린 문이 있다"
			CALL MESSAGE_WINDOW_D , "목소리" , "＞패스코드를 입력해주십시오" , "/" , "로그를 남긴다/버튼을 이용한다/재이용하지 않는다"
			CALL INPUT_MANY , 0 , 9999
			IF (DA:ARG:(ARG:1) == 11012 && RESULT == 9192) || (DA:ARG:(ARG:1) == 11022 && RESULT == 1213) || (DA:ARG:(ARG:1) == 11032 && RESULT == 1834)
				CALL MESSAGE_WINDOW_D , "" , "＞………/＞……/＞…"
				CALL MESSAGE_WINDOW_D , "목소리" , "＞패스코드를 확인/　잠금을 해제합니다"
				SETBIT 던전플래그:[[던전:팩토리]]:14 , DA:ARG:(ARG:1) / 10 % 10 -1
				RETURN 1
			ELSE
				CALL MESSAGE_WINDOW_D , "" , "＞………/＞……/＞…"
				CALL MESSAGE_WINDOW_D , "목소리" , "＞패스코드가 올바르지 않습니다"
				RETURN 0
			ENDIF
		ELSE
			RETURN 1
		ENDIF
		
	CASEELSE
		CALL CAN_ENTER,ARG,ARG:1
ENDSELECT
RETURN RESULT

