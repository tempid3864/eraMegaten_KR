;=======================================================================
;スタート地点입력
;=======================================================================
@START_DUNGEON_28,ARG
#LOCALSSIZE 1
FLAG:현M = 1
FLAG:현X = 1
FLAG:현Y = 1
;CALL AUTOMAP

;=======================================================================
;スタート時이벤트
;=======================================================================
@EVENT_DUNGEON_START_28
;フィールドマップを全アナライズ
IF 던전플래그:[[던전:팩토리]]:8 == 0
	FLAG:현M = 0
	CALL MAKE_FLOOR, 28
	FOR LOCAL , 0 , 31
		FOR LOCAL:1 , 0 , 41
			CALL SET_FLOOR_ANALYZE, 2 , [[던전:팩토리]] , 0 , LOCAL:1, LOCAL
		NEXT
	NEXT
	FLAG:현M = 1
	CALL MAKE_FLOOR, 28
	던전플래그:[[던전:팩토리]]:8 = 1
ENDIF

;키쿠에の상태。
IF 던전플래그:[[던전:팩토리]]:5 > -1 && 던전플래그:[[던전:팩토리]]:5 < 7 && 의뢰플래그:13:0 == 2
	SELECTCASE DAY - STRFLAG_NUM_REQ("의뢰를받은날" , , , , 13)
		;二日以内なら完全に無事
		CASE IS <= 2
			던전플래그:[[던전:팩토리]]:5 = 1
		;５日以内なら助かる
		CASE IS <= 5
			SIF 던전플래그:[[던전:팩토리]]:5 < 2
				던전플래그:[[던전:팩토리]]:5 = 2
		;１５日までは重度の洗脳
		CASE IS <= 15
			던전플래그:[[던전:팩토리]]:5 = 3
		;２５日までは洗脳完了ㆍ임신済みㆍ未출산
		CASE IS <= 25
			던전플래그:[[던전:팩토리]]:5 = 4
		;３５日までは半分牛ㆍ複数임신출산済みㆍ洗脳を解いても人格に影響
		CASE IS <= 35
			던전플래그:[[던전:팩토리]]:5 = 5
		;それ以後はただの牛
		CASEELSE
			던전플래그:[[던전:팩토리]]:5 = 6
		
	ENDSELECT
ENDIF

