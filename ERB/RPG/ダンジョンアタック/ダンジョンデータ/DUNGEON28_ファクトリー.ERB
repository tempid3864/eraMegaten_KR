@ENCOUNT_RATE_28
SELECTCASE FLAG:현M
	;팩토리
	CASE 0
		FLAG:인카운트율 = MAX(FLAG:미조우걸음수 * 3 - 15, 0)
		FLAG:MAG소비율 = 0
		FOR LOCAL , 1 , 7
			SIF POS(LOCAL) > -1
				PALAM:POS(LOCAL):욕정 += (ABL:POS(LOCAL):욕망+ 1) * 5
		NEXT
