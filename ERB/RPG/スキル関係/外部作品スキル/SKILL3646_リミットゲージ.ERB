@ACTION_3646,ARG,ARG:1
;撃ったらゲージを消し飛ばす
IF SKILLGAGE_H_GET(ARG,3646) == 100
	IF NO:ARG == [[캐릭터:프린스]]
		;単体デカジャ
		LOCAL = POS(ARG:1)
		FOR LOCAL:1,0,6
		CFLAG:LOCAL:(GET_BATTLESTATUS(LOCAL:1) + "강화") = MIN(0, CFLAG:LOCAL:(GET_BATTLESTATUS(LOCAL:1) + "강화"))
		NEXT
		CALL ATTACK_SINGLE,ARG,LOCAL,3646
		PRINTFORMW %CALLNAME:LOCAL%의 강화된 능력이 되돌아왔다！
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:워리어]]
		CFLAG:ARG:공격강화 = MIN(32, CFLAG:ARG:공격강화 + 12)
		CFLAG:ARG:명중강화 = MIN(32, CFLAG:ARG:명중강화 + 12)
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:팔랑크스]]
		CFLAG:POS(ARG:1):무효플래그 = 1
		CALL SKILL_WALL , ARG , "만능" , CFLAG:ARG:타겟 , 1
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:파이레츠]]
		CALL ATTACK_RAND,ARG,ARG:1,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:시노비]]
		CALL ATTACK_RAND,ARG,ARG:1,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:몽크]]
		CALL ATTACK_FIELD,ARG,ARG:1,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:조디악]]
		CALL ATTACK_RAND,ARG,ARG:1,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:비스트킹]]
		CALL ATTACK_FIELD,ARG,ARG:1,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:발리스타]]
		CFLAG:ARG:방어반감플래그 = 1
		LOCAL = POS(ARG:1)
		CALL ATTACK_SINGLE,ARG,LOCAL,3646
		CFLAG:ARG:방어반감플래그 = 0
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSEIF NO:ARG == [[캐릭터:파머]]
		CALL FIELD_BADSTATE,ARG,ARG:1,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ELSE
		CALL ATTACK_SINGLE,ARG,LOCAL,3646
		CALL SKILLGAGE_H_SET,ARG,3646,0
	ENDIF
ENDIF
;====================================================
;戦闘中スキルゲージの可視化
;====================================================
@SKILLGAGE_DISPLAY_3646,ARG
IF SKILLGAGE_H_GET(ARG,3646) < 100
	SETCOLOR 0xcc52b8
ELSEIF SKILLGAGE_H_GET(ARG,3646) == 100
	SETCOLOR 0xfffacd
ENDIF
PRINTFORML LIMITGAGE　{SKILLGAGE_H_GET(ARG,3646),3}％

RESETCOLOR

