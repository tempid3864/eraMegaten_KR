;====================================================
;行動によるスキルゲージ操作
;====================================================
@SKILLGAGE_CHARGE_3646,ARG,ARG:1
#LOCALSIZE 1
;自分の行動の場合
IF ARG == ARG:1
	;ゲージが貯まってないときにリミットスキルを使うといっぱい上がる
	SIF SKILLGAGE_H_GET(U,3646) < 100 && CFLAG:ARG:입력행동 == 3646
		CALL SKILLGAGE_H_ADD,ARG,3646,40
	
	;ダメージを受けた敵の数に応じてスキルゲージアップ
	FOR LOCAL,0, CHARANUM
		SIF CFLAG:LOCAL:PT플래그 == CFLAG:ARG:PT플래그
			CONTINUE
		IF CFLAG:LOCAL:현데미지 > 0
			CALL SKILLGAGE_H_ADD,ARG,3646,2
		ELSE
			CONTINUE
		ENDIF
		;会心、弱点を出すとさらにアップ
		SIF GETBIT(CFLAG:LOCAL:받음플래그,21)
			CALL SKILLGAGE_H_ADD,ARG,3646,3
		SIF GETBIT(CFLAG:LOCAL:받음플래그,22)
			CALL SKILLGAGE_H_ADD,ARG,3646,3
	NEXT
	;気合を入れても上がる
	SIF CFLAG:ARG:입력행동 == 511
		CALL SKILLGAGE_H_ADD,ARG,3646,20
	;集中しても上がる
	SIF CFLAG:ARG:입력행동 == 512
		CALL SKILLGAGE_H_ADD,ARG,3646,20
	;雄叫びしても上がる
	SIF CFLAG:ARG:입력행동 == 514
		CALL SKILLGAGE_H_ADD,ARG,3646,20
	;挑発しても上がる
	SIF CFLAG:ARG:입력행동 == 515
		CALL SKILLGAGE_H_ADD,ARG,3646,20
ENDIF
;ダメージを受けた又はカウンター、トリガー以外の攻撃を受けたら
;ゲージが増える
IF CFLAG:ARG:현데미지 > 0 || GETBIT(CFLAG:ARG:받음플래그,18) || GETBIT(CFLAG:ARG:받음플래그,19)
	LOCAL = 40 * CFLAG:ARG:현데미지 / MAXBASE:ARG:ＨＰ
	LOCAL = LOCAL < 2 ? 2 # LOCAL
	CALL SKILLGAGE_H_ADD,ARG,3646,LOCAL
ENDIF
;スキルゲージが100以上だと100にする
SIF SKILLGAGE_H_GET(ARG,3646) > 100
	CALL SKILLGAGE_H_SET,ARG,3646,100
RETURN 0

