
;이벤트플래그:106:0을 참조하는 부분
;32개(CASE 31)까지 작성가능

@EVENT_106_LIST_0(ARG,ARG:1)
;ARG:1 0 = 타이틀(표시되는 제목)	*반환값 없음
;　　　1 = 출현체크
;　　　2 = 이벤트(호출부)			*반환값 없음
;
SELECTCASE ARG
CASE 0 ;세라픽게이트
	IF ARG:1 == 0
		IF 세라픽게이트진행도 == 0
			RESULTS = 【EX:발키리 프로파일】차원 게이트 출현

		ELSEIF 세라픽게이트진행도 == 1
			RESULTS = 【EX:발키리 프로파일】세라픽 게이트 녹색 에리어 최심부

		ELSEIF 세라픽게이트진행도 == 2
			RESULTS = 【EX:발키리 프로파일】세라픽 게이트 백색 에리어 최심부
		ENDIF
		
	ELSEIF ARG:1 == 1
		SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
			RETURN 0

		IF 세라픽게이트진행도 == 0 && FLAG:경험ED수 > 4 && BASE:MASTER:LV >= 60
			RETURN 1
		ELSEIF 세라픽게이트진행도 == 1
			RETURN 1
		ELSEIF 세라픽게이트진행도 == 2
			RETURN 1
		ELSE
			RETURN 0
		ENDIF

	ELSEIF ARG:1 == 2
		CALL 세라픽게이트
	ENDIF

CASE 1 ;단죄의황녀
	IF ARG:1 == 0
		RESULTS = 【EX:원신】단죄의 황녀

	ELSEIF ARG:1 == 1
		SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
			RETURN 0

		SIF BASE:MASTER:LV >= 20
			RETURN 1
		
		RETURN 0
	
	ELSEIF ARG:1 == 2
		CALL 단죄의황녀
	ENDIF

CASE 2 ;일그러진 도쿄
	IF ARG:1 == 0;타이틀
		IF 일그러진도쿄진행도 == 0
			RESULTS = [일그러진 도쿄] 미나미스나마치 이변 조사

		ELSEIF 일그러진도쿄진행도 == 1
			RESULTS = [일그러진 도쿄] 마인의 연주

		ELSEIF 일그러진도쿄진행도 == 2
			RESULTS = [일그러진 도쿄] 즉신마인

		ELSEIF 일그러진도쿄진행도 == 3
			RESULTS = [일그러진 도쿄] 마인의 질주

		ELSEIF 일그러진도쿄진행도 == 4
			RESULTS = [일그러진 도쿄] 살육마인방송

		ELSEIF 일그러진도쿄진행도 == 5
			RESULTS = [일그러진 도쿄] 소녀마인

		ELSEIF 일그러진도쿄진행도 == 6
			RESULTS = [일그러진 도쿄] 구세마인
		ENDIF

	ELSEIF ARG:1 == 1;출현체크

		IF 일그러진도쿄진행도 == 0 && BASE:MASTER:LV >= 70
			RETURN 1
		ELSEIF 일그러진도쿄진행도 == 1
			RETURN 1
		ELSEIF 일그러진도쿄진행도 == 2
			RETURN 1
		ELSEIF 일그러진도쿄진행도 == 3
			RETURN 1
		ELSEIF 일그러진도쿄진행도 == 4
			RETURN 1
		ELSEIF 일그러진도쿄진행도 == 5
			RETURN 1
		ELSEIF 일그러진도쿄진행도 == 6
			RETURN 1
		ENDIF

		RETURN 0
	
	ELSEIF ARG:1 == 2;이벤트(호출부)
		IF 일그러진도쿄진행도 == 0
			CALL EVENT_106_2_0

		ELSEIF 일그러진도쿄진행도 == 1
			CALL EVENT_106_2_1

		ELSEIF 일그러진도쿄진행도 == 2
			CALL EVENT_106_2_2
		
		ELSEIF 일그러진도쿄진행도 == 3
			CALL EVENT_106_2_3

		ELSEIF 일그러진도쿄진행도 == 4
			CALL EVENT_106_2_4

		ELSEIF 일그러진도쿄진행도 == 5
			CALL EVENT_106_2_5

		ELSEIF 일그러진도쿄진행도 == 6
			CALL EVENT_106_2_6

		ENDIF
	ENDIF

CASE 3 ;디지몬 모드
	IF ARG:1 == 0
		IF 디지몬이벤트진행도 == 0
			RESULTS = Into the Coulomb

		ELSEIF 디지몬이벤트진행도 == 1
			RESULTS = Halfwit Girl

		ELSEIF 디지몬이벤트진행도 == 2
			RESULTS = 캐릭터추가테스트

		ENDIF

	ELSEIF ARG:1 == 1
		SIF GETBIT(FLAG:EXTRA이벤트설정, 0) == 0
			RETURN 0

		IF 디지몬이벤트진행도 == 0 && BASE:MASTER:LV >= 15
			RETURN 1
		ELSEIF 디지몬이벤트진행도 == 1 && BASE:MASTER:LV >= 30
			RETURN 1
		ELSEIF 디지몬이벤트진행도 == 2
			RETURN 1
		ENDIF

		RETURN 0
	
	ELSEIF ARG:1 == 2
		IF 디지몬이벤트진행도 == 0
			CALL EVENT_106_3_0_Into_the_Coulomb

		ELSEIF 디지몬이벤트진행도 == 1
			CALL EVENT_106_3_1_Halfwit_Girl

		ELSEIF 디지몬이벤트진행도 == 2
			CALL EVENT_106_3_X_캐릭터추가테스트

		ENDIF
	ENDIF

CASEELSE
	RETURN 0
ENDSELECT
RETURN 0